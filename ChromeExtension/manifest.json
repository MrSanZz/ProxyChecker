{
  "manifest_version": 3,
  "name": "Proxy Applier",
  "version": "1.2",
  "description": "Apply and manage proxy settings in Google Chrome",
  "permissions": ["proxy", "storage"],
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html"
  }
}
